<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/loginMobileBankingFragment">
    <action
        android:id="@+id/actionHome_to_transferActivity"
        app:destination="@id/transferActivity" />
    <action
        android:id="@+id/action_global_historyMobileBankingFragment"
        app:destination="@id/historyMobileBankingFragment" />
    <action
        android:id="@+id/action_global_homeMobileBankingFragment"
        app:destination="@id/homeMobileBankingFragment" />
    <fragment
        android:id="@+id/loginMobileBankingFragment"
        android:name="com.app.livecode.mobilebanking.fragments.LoginMobileBankingFragment"
        android:label="fragment_login_mobile_banking"
        tools:layout="@layout/fragment_login_mobile_banking"></fragment>
    <fragment
        android:id="@+id/homeMobileBankingFragment"
        android:name="com.app.livecode.mobilebanking.fragments.HomeMobileBankingFragment"
        android:label="fragment_home_mobile_banking"
        tools:layout="@layout/fragment_home_mobile_banking" />
    <fragment
        android:id="@+id/historyMobileBankingFragment"
        android:name="com.app.livecode.mobilebanking.fragments.HistoryMobileBankingFragment"
        android:label="fragment_history_mobile_banking"
        tools:layout="@layout/fragment_history_mobile_banking" />
    <activity
        android:id="@+id/transferActivity"
        android:name="com.app.livecode.mobilebanking.TransferActivity"
        android:label="activity_transfer"
        tools:layout="@layout/activity_transfer" />
</navigation>